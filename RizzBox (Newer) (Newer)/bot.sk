options:
    srmodLimit: 3
    adminLimit: 5
    # When changing the limt you MUST reset current cooldowns (will be updated later)
    prefix: &f[&bIRP Protection&f]&7
    cooldown: 24 # hours
    embedImage: https://cdn.discordapp.com/attachments/1110988688617377794/1141470447859417138/RizzBoxWithGlow.png
    alertsChannel: 1125557663799971933
    alertsPing: 1013682377198940187
    embedColor: gold


command refundmanager [<text>] [<text>]:
    permission: op
    aliases: /refundman
    trigger:
        if arg-1 is "resetall":
            set {refunds::*} to 0
            send "{@prefix} Reset all refund cooldowns."
        if arg-1 is "check":
            if arg-2 is set:
                send "{@prefix} %(arg-2 parsed as a player)% has used %{refunds::%(arg-2 parsed as a player)'s uuid%}% total refund(s)."
        if arg-1 is "resetplayer":
            if arg-2 is set:
                send "{@prefix} Reset %(arg-2 parsed as a player)%'s refund cooldowns"
                set {refunds::%(arg-2 parsed as a player)'s uuid%} to 0
        else if arg-1 isn't set:
            send " "
            send "&e&lRefund Protection"
            send "&a/refundman <resetall> &7&o(Resets all refund cooldowns)"
            send "&a/refundman <check> <player> &7&o(Checks a players refund count)"
            send "&a/refundman <resetplayer> <player> &7&o(Resets the specified player refunds)"
            send " "

on tab complete:
    event-string is "/refundmanager" or "/refundman"
    set tab completions for position 1 to "resetall", "resetplayer" and "check"

on command "/ir restore":
    set {_refundAlerts} to role with the id "{@alertsPing}"
    if player's group is "srmod":
        if {refunds::%player's uuid%} >= {@srmodLimit}:
            send "{@prefix} &cYou cannot refund more than {@srmodLimit} times per day. Please contact server admins if you believe this is a mistake."
            cancel event
            make embed:
                set embed color of embed to {@embedColor}
                set title of embed to "__Refund Attempt__"
                set description of embed to "%player% was denied refund permissions, as they have already used the max limit (%{refunds::%player's uuid%}%)."
                set timestamp of embed to now
                set thumbnail of embed to "{@embedImage}"
            post last embed to channel with id "{@alertsChannel}"
            post mention tag of {_refundAlerts} to text channel with id "{@alertsChannel}"
        else:
            add 1 to {refunds::%player's uuid%}
            make embed:
                set title of embed to "__Player Refunded__ (Sr-Mod)"
                set embed color of embed to {@embedColor}
                set description of embed to "%player% has just used 1 of their refund limits. They now have %{refunds::%player's uuid%}% total refunds today. %nl%**Full Command: __/%full command%__**"
                set timestamp of embed to now
                set thumbnail of embed to "{@embedImage}"
            post last embed to channel with id "{@alertsChannel}"
            
        
    if player's group is "admin":
        if {refunds::%player's uuid%} >= {@adminLimit}:
            send "{@prefix} &cYou cannot refund more than {@adminLimit} times per day. Please contact server admins if you believe this is a mistake."
            cancel event
            make embed:
                set embed color of embed to {@embedColor}
                set title of embed to "__Refund Attempt__"
                set description of embed to "%player% was denied refund permissions, as they have already used the max limit (%{refunds::%player's uuid%}%)."
                set timestamp of embed to now
                set thumbnail of embed to "{@embedImage}"
            post last embed to channel with id "{@alertsChannel}"
            post mention tag of {_refundAlerts} to text channel with id "{@alertsChannel}"
        else:
            add 1 to {refunds::%player's uuid%}
            make embed:
                set title of embed to "__Player Refunded__ (Admin)"
                set embed color of embed to {@embedColor}
                set description of embed to "%player% has used there %{refunds::%player's uuid%}% /5 refund. %nl%**Full Command: __/%full command%__**"
                set timestamp of embed to now
                set thumbnail of embed to "{@embedImage}"
            post last embed to channel with id "{@alertsChannel}"

every {@cooldown} hours:
    set {refunds::*} to 0


